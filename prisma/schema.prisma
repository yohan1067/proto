generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  kakaoId   BigInt   @unique
  email     String?  @unique
  nickname  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
